package nestedloop;

public class AlphabetLetter_OPQ {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		letterAlphabet(7);

	}

	public static void letterAlphabet(int n) {
		// TODO Auto-generated method stub
		for(int i=0;i<=6;i++) {
			for(int j=0;j<=i;j++) {
				if(((i==0 && i<6)&&(j==0||j==6)) || ((i==0||i<=6))){
					System.out.print("O"+" ");
				}
				System.out.print(" ");
			}
		}
		System.out.println();
	}

}
